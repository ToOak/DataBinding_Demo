<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zjun.databinding.demo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FastUseActivity" />
        <activity android:name=".IncludeParamsPostActivity" />
        <activity
            android:name=".ListShowActivity"
            android:label="@string/title_activity_list_show"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ImageViewActivity" />
        <activity android:name=".ListViewActivity" />
        <activity android:name=".ListViewUpdateActivity" />
        <activity android:name=".NotStaticActivity" />
        <activity android:name=".FragmentShowActivity" />
        <activity android:name=".RecyclerViewActivity"></activity>
    </application>

</manifest>